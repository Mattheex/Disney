      {
        "titles": "tom_aud_score",
        "suppressOutput": true
      },
      {
        "titles": "tom_ratings",
        "suppressOutput": true
      },
      {
        "titles": "entity",
        "propertyUrl": "http://projet.fr/films/entity"
      },
      {
        "titles": "id",
        "propertyUrl": "rdf:type",
        "valueUrl": "http://projet.fr/films/CreativeWork",
        "required": true
