      {
        "titles": "tom_aud_score",
        "suppressOutput": true
      },
      {
        "titles": "tom_ratings",
        "suppressOutput": true
      },
      {
        "titles": "entity",
        "propertyUrl": "http://projet.fr/films_schema/entity"
      },
      {
        "titles": "id",
        "propertyUrl": "rdf:type",
        "valueUrl": "schema:movie",
        "required": true
