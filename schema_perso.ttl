PREFIX owl:  <http://www.w3.org/2002/07/owl#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX sh:   <http://www.w3.org/ns/shacl#>
PREFIX film: <http://projet.fr/films_schema#>
PREFIX :     <http://projet.fr/perso_schema#>
PREFIX xsd:  <http://www.w3.org/2001/XMLSchema#> .

:Person
    a          owl:Class ;
    rdfs:label "person" .

:Fictional
    a          owl:Class ;
    rdfs:label "fictionnal" .

:Real
    a          owl:Class ;
    rdfs:label "real" .

:Actor
    a                  owl:Class ;
    rdfs:label         "an actor" ;
    owl:intersectionOf ( :Person :Real ) .

:Director
    a                  owl:Class ;
    rdfs:label         "a director" ;
    owl:intersectionOf ( :Person :Real ) .

:Character
    a                  owl:Class ;
    rdfs:label         "a character" ;
    owl:intersectionOf ( :Person :Fictional ) .

:hasImage
    a          owl:ObjectProperty ;
    rdfs:label "has image" .

:name
    a           owl:DatatypeProperty ;
    rdfs:label  "name" ;
    rdfs:domain :Person ;
    rdfs:range  xsd:string .

:id
    a          owl:DatatypeProperty ;
    rdfs:label "id" .

:heroName a owl:DatatypeProperty;
    rdfs:label "Full-name";
    rdfs:comment "Hero/Villain name".

:alias a owl:DatatypeProperty;
    rdfs:label "Alias".

:alter a owl:DatatypeProperty;
    rdfs:label "Alter-egos".

:placeOfBirth a owl:DatatypeProperty;
    rdfs:label "Place of birth".

:Male a owl:Class;
    rdfs:label "Male".

:Female a owl:Class;
    rdfs:label "Female".

:eyes a owl:DatatypeProperty;
    rdfs:label "Color eyes".

:hair a owl:DatatypeProperty;
    rdfs:label "Color hair".

:apparition a owl:DatatypeProperty;
    rdfs:label "first apparition".

:Villain a owl:Class;
    rdfs:label "Villain".

:Hero a owl:Class;
    rdfs:label "Hero".

:heightF a owl:DatatypeProperty;
    rdfs:label "height in feet".

:heightCm a owl:DatatypeProperty;
    rdfs:label "height in cm".

:weightKg a owl:DatatypeProperty;
    rdfs:label "weight in kg ".

:weightLb a owl:DatatypeProperty;
    rdfs:label "weight in lb ".

:base a owl:DatatypeProperty;
    rdfs:label "Base".

:affiliation a owl:DatatypeProperty;
    rdfs:label "group-affiliation".

:relatives a owl:DatatypeProperty;
    rdfs:label "relatives".

:intelligence a owl:DatatypeProperty;
    rdfs:label "intelligence".
        
:strength a owl:DatatypeProperty;
    rdfs:label "strength".
        
:speed a owl:DatatypeProperty;
    rdfs:label "speed".
        
:durability a owl:DatatypeProperty;
    rdfs:label "durability".
        
:power a owl:DatatypeProperty;
    rdfs:label "power".
        
:combat a owl:DatatypeProperty;
    rdfs:label "combat".

:publisher a owl:DatatypeProperty;
    rdfs:label "publisher".

#publisher occupation
